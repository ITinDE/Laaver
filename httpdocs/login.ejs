<body>


    <main class="main_wrapper body__overlay overflow__hidden">

		<%- include('./header.ejs') %>   

		<!-- login__section__start -->
		<div class="loginarea  sp_bottom_80 sp_top_80">
		 <div class="container">
			<div class="row">
				<div class="tab-content tab__content__wrapper">

					<div class="tab-pane fade active show" id="projects__two" role="tabpanel" aria-labelledby="projects__two">
					  <div class="col-xl-8 offset-md-2 loginarea__col">
						<div class="loginarea__wraper">
						  <div class="loginarea__heading">
							<h5 class="login__title" id="login_title">Login</h5>
							<p class="login__description" id="login_text_noaccount">
							  Don't have an account yet?
							  <a href="/register" id="login_link_signup">Sign up for free</a>
							</p>
						  </div>

						  <form action="#">
							<div class="loginarea__form">
							  <label class="form__label" id="login_label_username_email">Username or email</label>
							  <input class="common__login__input" type="text" placeholder="Your username or email" id="username">
							</div>
							<div class="loginarea__form">
							  <label class="form__label" id="login_label_password">Password</label>
							  <input class="common__login__input" type="password" placeholder="Password" id="password">
							</div>
							<div class="loginarea__form d-flex justify-content-between flex-wrap gap-2">
							  <div class="form__check">
								<input type="checkbox" id="login__privacy">
								<label for="login__privacy" id="login_label_remember">Remember Me</label>
							  </div>
							  <div class="text-end login__form__link">
								<a href="/passwort_forgot" id="login_link_forgot">Forgot your password?</a>
							  </div>
							</div>
							<div class="loginarea__button text-center">
							  <a class="default__button" href="javascript:checkLoginInfo()" id="login_button_submit">Log In</a>
							</div>
						  </form>
						</div>
					  </div>
					</div>


				</div>
			</div>
		</div>
		</div>
		
		<!-- Hinweisen Modal Start -->
		<div class="modal fade theme-modal deal-modal" id="HinweisenModalFade" tabindex="-1">
			<div class="modal-dialog modal-dialog-centered modal-fullscreen-sm-down">
				<div class="modal-content">
					<div class="modal-header">
						<div>
							<h5 class="modal-title w-100" id="HinweisenModalTitle"></h5>
						</div>
						<button type="button" class="btn-close" data-bs-dismiss="modal" id="HinweisenModalButton1"></button>
					</div>
					<div class="modal-body">
						<div id="HinweisenModalInhalt" style="font-size:16px;margin:5px 5px 20px 5px;"></div>					
						<div>
							<button class="btn btn-md fw-bold mt-md-4 mt-2 mend-auto" id="HinweisenModalButton2" style="width:100%;" data-bs-dismiss="modal">
								<span id="modal_button_close">Schließen</span>
							</button>
						</div>
					</div>

				</div>
			</div>
		</div>
		<!-- Hinweisen Modal End -->



	    <%- include('./footer.ejs') %>     

	</main>

	<script>
		languageInnerHTMLArr.push(new languageObj("login_title", "Login", "Anmelden", "Connexion", "登录"));
		languageInnerHTMLArr.push(new languageObj("login_text_noaccount", "Don't have an account yet?", "Sie haben noch kein Konto?", "Vous n'avez pas encore de compte ?", "还没有账户？"));
		languageInnerHTMLArr.push(new languageObj("login_link_signup", "Sign up for free", "Kostenlos registrieren", "Inscrivez-vous gratuitement", "免费注册"));
		languageInnerHTMLArr.push(new languageObj("login_label_username_email", "Username or email", "Benutzername oder E-Mail", "Nom d'utilisateur ou e-mail", "用户名或邮箱"));
		languageInnerHTMLArr.push(new languageObj("login_label_password", "Password", "Passwort", "Mot de passe", "密码"));
		languageInnerHTMLArr.push(new languageObj("login_label_remember", "Remember Me", "Angemeldet bleiben", "Se souvenir de moi", "记住我"));
		languageInnerHTMLArr.push(new languageObj("login_link_forgot", "Forgot your password?", "Passwort vergessen?", "Mot de passe oublié ?", "忘记密码？"));
		languageInnerHTMLArr.push(new languageObj("login_button_submit", "Log In", "Anmelden", "Se connecter", "登录"));

		languagePlaceholderArr.push(new languageObj("username", "Your username or email", "Ihr Benutzername oder Ihre E-Mail", "Votre nom d'utilisateur ou e-mail", "您的用户名或邮箱"));
		languagePlaceholderArr.push(new languageObj("password", "Password", "Passwort", "Mot de passe", "密码"));


	</script>

