<!doctype html>
<html class="no-js layout__7" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Laaver</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" type="image/x-icon" href="images/favicon.ico">
    <!-- Place favicon.ico in the root directory -->

        <!-- CSS here -->
        <link rel="stylesheet" href="css/bootstrap.min.css">
        <link rel="stylesheet" href="css/animate.min.css">
        <link rel="stylesheet" href="css/magnific-popup.css">
        <link rel="stylesheet" href="css/all.min.css">
        <link rel="stylesheet" href="css/futura-std-4.css">
        <link rel="stylesheet" href="css/slick.css">
        <link rel="stylesheet" href="css/style.css">		

		<script src="javascript/header_footer.js "></script>
</head>


<body>


    <main class="main_wrapper body__overlay overflow__hidden">

		<%- include('./header.ejs') %>  

		
		<!-- cart__section__start -->
		<div class="cartarea sp_bottom_100 sp_top_100">
			<div class="container">
				<div class="row">
					<div class="col-lg-9 col-12">
						<form action="#">
							<div class="cartarea__table__content">
								<table style="width:100%;">
									<thead>
										<tr>
											<th id="cart_table_image">Image</th>
											<th id="cart_table_product">Product</th>
											<th id="cart_table_price">Price</th>
											<th id="cart_table_quantity">Quantity</th>
											<th id="cart_table_total">Total</th>
											<th id="cart_table_remove">Remove</th>
										</tr>
									</thead>
									<tbody id="mainTableDiv"></tbody>
								</table>
							</div>
						</form>
					</div>			
					<div class="col-lg-3 col-12">
						<div class="cartarea__grand__totall cartarea__tax">
							<div class="cartarea__title">
								<h4 id="summary_title">Übersicht</h4>

							</div>
							<table style="width: 100%; border-collapse: collapse; margin:20px 0 10px 0;">
								<tr>
									<td style="text-align: left;"><span id="summary_total_label">Gesamtbetrag</span></td>
									<td style="text-align: right;">
										<h6 id="PreisDiv1" style="margin:0;"></h6>
										<h6 id="PreisDiv3" style="margin:0;display:none;"></h6>
									</td>
								</tr>
							</table>
							<a class="default__button" href="javascript:saveEinkaufswagen()" id="goToCheckoutButton" style="width:100%;">Proceed to Checkout</a>
						</div>
					</div>
				</div>
			</div>
		</div>
        <!-- cart__section__end-->
		
		<!-- Hinweisen Modal Start -->
		<div aria-modal="true" class="modal theme-modal fade" id="HinweisenModalFade" role="dialog" tabindex="-1">
			<div class="modal-dialog modal-lg modal-dialog-centered" role="document">
				<div class="modal-content">
					<div class="modal-header">
						<div>
							<h5 class="modal-title w-100" id="HinweisenModalTitle"></h5>
						</div>
						<button type="button" class="btn-close" data-bs-dismiss="modal" id="HinweisenModalButton1"></button>
					</div>
					<div class="modal-body">
						<div id="HinweisenModalInhalt" style="font-size:16px;margin:5px 5px 20px 5px;"></div>					
						<div>
							<button class="btn btn-md text-white fw-bold mt-md-4 mt-2 mend-auto" id="HinweisenModalButton2" style="width:100%;" data-bs-dismiss="modal">
								<span style="color:#FFF;">Schließen</span>
							</button>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- Hinweisen Modal End -->

	    <%- include('./footer.ejs') %>     
		

	</main>

	<script>
		languageInnerHTMLArr.push(new languageObj("cart_table_image", "Image", "Bild", "Image", "图片"));
		languageInnerHTMLArr.push(new languageObj("cart_table_product", "Product", "Produkt", "Produit", "产品"));
		languageInnerHTMLArr.push(new languageObj("cart_table_price", "Price", "Preis", "Prix", "价格"));
		languageInnerHTMLArr.push(new languageObj("cart_table_quantity", "Quantity", "Menge", "Quantité", "数量"));
		languageInnerHTMLArr.push(new languageObj("cart_table_total", "Total", "Gesamt", "Total", "总计"));
		languageInnerHTMLArr.push(new languageObj("cart_table_remove", "Remove", "Entfernen", "Supprimer", "移除"));
		languageInnerHTMLArr.push(new languageObj("summary_title", "Overview", "Übersicht", "Aperçu", "概览"));
		languageInnerHTMLArr.push(new languageObj("summary_total_label", "Total Amount", "Gesamtbetrag", "Montant total", "总金额"));
		languageInnerHTMLArr.push(new languageObj("goToCheckoutButton", "Proceed to Checkout", "Zur Kasse", "Passer à la caisse", "前往结账"));


	</script>

    <!-- JS here -->
