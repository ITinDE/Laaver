		function generateNewRow(param) {
			let colClass = "12";
			if (param == "ProduktKatalogFloat10" || param == "ProduktKatalogFloat11") {
				colClass = "6";
			}else if (param == "ProduktKatalogInt1") {
				colClass = "6";
			}
			return colClass;
		}
		function generateNewType(param) {
			let txt = "text";
			if (param.indexOf("Int")>0 || param.indexOf("Float")>0) {
				txt = "number";
			}else if (param.indexOf("Datum")>=0) {
				txt = "date";
			}
			return txt;
		}
